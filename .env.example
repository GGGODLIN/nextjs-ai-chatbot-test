# Get your OpenAI API Key here for chat models: https://platform.openai.com/account/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Get your Fireworks AI API Key here for reasoning models: https://fireworks.ai/account/api-keys
FIREWORKS_API_KEY=your_fireworks_api_key_here

# Generate a random secret: https://generate-secret.vercel.app/32 or `openssl rand -base64 32`
AUTH_SECRET=your_auth_secret_here

# The following keys below are automatically created and
# added to your environment when you deploy on vercel

# Instructions to create a Vercel Blob Store here: https://vercel.com/docs/storage/vercel-blob
BLOB_READ_WRITE_TOKEN=your_blob_read_write_token_here

# Database connection URLs (Postgres)
DATABASE_URL=postgres://username:password@host-pooler.region.provider.com/dbname?sslmode=require
DATABASE_URL_UNPOOLED=postgresql://username:password@host.region.provider.com/dbname?sslmode=require
POSTGRES_URL=postgres://username:password@host-pooler.region.provider.com/dbname?sslmode=require
POSTGRES_URL_NON_POOLING=postgres://username:password@host.region.provider.com/dbname?sslmode=require
POSTGRES_URL_NO_SSL=postgres://username:password@host-pooler.region.provider.com/dbname
POSTGRES_PRISMA_URL=postgres://username:password@host-pooler.region.provider.com/dbname?pgbouncer=true&connect_timeout=15&sslmode=require

# Database connection details
PGDATABASE=dbname
PGHOST=host-pooler.region.provider.com
PGHOST_UNPOOLED=host.region.provider.com
PGPASSWORD=your_password_here
PGUSER=username
POSTGRES_DATABASE=dbname
POSTGRES_HOST=host-pooler.region.provider.com
POSTGRES_PASSWORD=your_password_here
POSTGRES_USER=username

# Additional AI API keys
GOOGLE_GENERATIVE_AI_API_KEY=your_google_ai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
DEEPSEEK_API_KEY=your_deepseek_api_key_here
XAI_API_KEY=your_xai_api_key_here
